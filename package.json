{
  "name": "nestjs-warthog",
  "version": "0.1.0",
  "description": "NestJS module to generate (scalar) type-graphql inputs from typeORM entities. Based on warthog.dev",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "install-peers"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.5.1",
    "@nestjs/typeorm": "^6.1.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "graphql": "^14.5.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.19"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.7.2",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "24.0.18",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "12.7.5",
    "@types/prettier": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "dotenv": "^8.1.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "faker": "^4.1.0",
    "install-peers-cli": "^2.1.1",
    "jest": "24.9.0",
    "pg": "^7.12.1",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "^3.7.0-dev.20190925"
  }
}
